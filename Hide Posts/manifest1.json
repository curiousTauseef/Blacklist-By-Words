{
    "name": "FB-Hide",
    "version": "0.1.0",
    "description": "Hide posts that you don't want in your Facebook feed",
    "browser_action": {
      "default_icon": "icons/icon48.png",
      "default_popup": "Keywords.html"
    },
    "icons": {
	     "16": "./icons/icon16.png",
        "48": "./icons/icon48.png",
        "128": "./icons/icon128.png" },
    "options_page": "Keywords.html",
    "permissions": ["storage"],
    "manifest_version": 2
}
