{
    "name": "Hide Facebook posts by keyword",
    "version": "0.1.0",
    "description": "Hide posts that you don't want in your Facebook feed",
    "icons": {
	"16": "./icons/icon16.png",
        "48": "./icons/icon48.png",
        "128": "./icons/icon128.png" },
    "content_scripts": [{
        "matches": ["*://*.facebook.com/*"],
        "css": ["HidePost.css"],
        "js": ["jquery-1.9.0.min.js", "HidePost.js"]
    }],
    "options_page": "Keywords.html",
    "permissions": ["storage"],
    "manifest_version": 2
}
